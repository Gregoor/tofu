{
  "name": "tofu",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "parcel --no-hmr src/index.html",
    "deploy": "git stash && git branch -f gh-pages && git checkout gh-pages && parcel build src/index.html --public-url /tofu/ && cp -r dist/* . && git add -A . && git commit -am. && git push origin gh-pages --force && git checkout master && git stash apply",
    "test": "tsc --noEmit && uvu -r ts-node/register",
    "watch:tsc": "tsc --noEmit --watch",
    "watch:test": "watchlist src -- uvu -r ts-node/register"
  },
  "dependencies": {
    "@babel/generator": "^7.11.6",
    "@babel/parser": "^7.11.5",
    "@babel/types": "^7.11.5",
    "@types/lodash.pick": "^4.4.6",
    "@types/memoize-one": "^5.1.2",
    "@types/prettier": "^2.1.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/redom": "^3.23.1",
    "@types/styled-components": "^5.1.3",
    "codeflask": "^1.4.1",
    "downshift": "^6.0.6",
    "immer": "^7.0.9",
    "lodash.pick": "^4.4.0",
    "memoize-one": "^5.1.1",
    "prettier": "^2.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@types/node": "^14.11.2",
    "parcel": "2.0.0-beta.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "uvu": "^0.3.3",
    "watchlist": "^0.2.3"
  },
  "engines": {
    "node": ">=8.12.0"
  },
  "browserslist": [
    "last 2 firefox versions",
    "last 2 chrome versions",
    "last 1 edge version",
    "last 1 safari version"
  ]
}
