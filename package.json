  "engines": {
    "node": ">=8.12.0"
  },
  "browserslist": [
    "last 1 chrome version",
    "last 1 firefox version",
    "last 1 safari version"
  ],
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "eqeqeq": "off"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "workerize-loader(\\?.*)?!(.*)": "$2"
    }
  },
  "importSort": {
    ".js, .jsx, .es6, .es, .mjs, .ts, .tsx": {
      "parser": "babylon",
      "style": "module"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
