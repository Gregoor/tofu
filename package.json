{
  "name": "tofu",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "parcel --no-hmr src/index.html",
    "deploy": "git stash && git branch -f gh-pages && git checkout gh-pages && parcel build src/index.html --public-url /tofu/ && cp -r dist/* . && git add -A . && git commit -am. && git push origin gh-pages --force && git checkout master && git stash apply",
    "test": "tsc --noEmit && uvu -r ts-node/register",
    "watch:tsc": "tsc --noEmit --watch",
    "watch:test": "watchlist src -- uvu -r ts-node/register"
  },
  "dependencies": {
    "@babel/generator": "^7.12.5",
    "@babel/parser": "^7.12.5",
    "@babel/types": "^7.12.6",
    "@emotion/core": "^10.1.1",
    "@emotion/styled": "^10.0.27",
    "@types/lodash.pick": "^4.4.6",
    "@types/memoize-one": "^5.1.2",
    "@types/prettier": "^2.1.5",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/redom": "^3.23.1",
    "codeflask": "^1.4.1",
    "downshift": "^6.0.6",
    "emotion-theming": "^10.0.27",
    "immer": "^7.0.14",
    "lodash.pick": "^4.4.0",
    "memoize-one": "^5.1.1",
    "prettier": "^2.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.6",
    "import-sort-style-module": "^6.0.0",
    "parcel": "2.0.0-beta.1",
    "prettier-plugin-import-sort": "^0.0.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "uvu": "^0.4.0",
    "watchlist": "^0.2.3"
  },
  "engines": {
    "node": ">=8.12.0"
  },
  "browserslist": [
    "last 2 firefox versions",
    "last 2 chrome versions",
    "last 1 edge version",
    "last 1 safari version"
  ],
  "importSort": {
    ".js, .jsx, .es6, .es, .mjs, .ts, .tsx": {
      "parser": "babylon",
      "style": "module"
    }
  }
}
